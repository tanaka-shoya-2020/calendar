<%= render 'layouts/sidebar' %>
<div class="col-sm-9">
  <h1 class="text-center">カレンダー</h1>
  <%= month_calendar events: @events do |date,events| %>
    <%= date.day %>
    <% events.each do |event| %>
      <div>
        <%= link_to "#{event.title}", sample_path(event) %>
      </div>
    <% end %>
  <% end %>
</div>